{
    "name": "@frontify/arcade",
    "version": "0.7.3",
    "description": "Design system of Frontify",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        },
        "./config": {
            "import": "./dist/config.es.js",
            "require": "./dist/config.umd.js"
        },
        "./style": "./dist/index.css"
    },
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "npm run clean && npm run generate && cross-env NODE_ENV=production npm run build:library && cross-env NODE_ENV=production npm run build:config",
        "build:config": "rollup -c",
        "build:css": "tailwindcss -i ./src/styles.css -o dist/index.css",
        "build:library": "vite build",
        "build:storybook": "build-storybook -s .storybook/assets",
        "dev": "cross-env NODE_ENV=development npm run dev:library & cross-env NODE_ENV=development npm run dev:config",
        "dev:config": "rollup -c -w",
        "dev:library": "vite build --watch",
        "clean": "rimraf dist",
        "component:create": "esno scripts/createNewComponent.ts",
        "generate": "esno scripts/generateExports.ts",
        "lint": "npm run lint:ts",
        "lint:fix": "npm run lint:ts:fix",
        "lint:ts": "eslint 'src/**/*.{ts,tsx}'",
        "lint:ts:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
        "lint:prettier": "prettier --check src",
        "test": "npm run build:css && cypress run-ct",
        "test:browser": "npm run build:css && cypress open-ct",
        "test:file": "npm run build:css && cypress run-ct --spec",
        "typecheck": "tsc",
        "storybook": "start-storybook -p 6006 -s .storybook/assets",
        "prepublishOnly": "npm run build",
        "prepare": "is-ci || husky install"
    },
    "keywords": [
        "frontify",
        "react",
        "components",
        "design system"
    ],
    "author": "Frontify Developers <developers@frontify.com>",
    "repository": {
        "url": "https://github.com/frontify/arcade"
    },
    "bugs": {
        "url": "https://github.com/frontify/arcade/issues"
    },
    "devDependencies": {
        "@cypress/react": "5.10.1",
        "@cypress/vite-dev-server": "2.1.1",
        "@frontify/eslint-config-react": "0.11.0",
        "@rollup/plugin-alias": "3.1.5",
        "@rollup/plugin-commonjs": "21.0.0",
        "@rollup/plugin-node-resolve": "13.0.5",
        "@storybook/addon-actions": "6.3.12",
        "@storybook/addon-essentials": "6.3.12",
        "@storybook/addon-links": "6.3.12",
        "@storybook/addon-postcss": "2.0.0",
        "@storybook/react": "6.3.12",
        "@svgr/core": "5.5.0",
        "@svgr/plugin-jsx": "5.5.0",
        "@svgr/plugin-prettier": "5.5.0",
        "@svgr/plugin-svgo": "5.5.0",
        "@types/draft-js": "0.11.6",
        "@types/react": "17.0.30",
        "@types/react-dom": "17.0.9",
        "@vitejs/plugin-react-refresh": "1.3.6",
        "autoprefixer": "10.3.7",
        "chalk": "4.1.2",
        "cross-env": "7.0.3",
        "cypress": "8.6.0",
        "cypress-real-events": "1.5.1",
        "esbuild": "0.13.6",
        "eslint": "7.32.0",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "esno": "0.10.1",
        "fast-glob": "3.2.7",
        "husky": "7.0.2",
        "is-ci": "3.0.0",
        "postcss": "8.3.9",
        "postcss-cli": "9.0.1",
        "postcss-nested": "5.0.6",
        "prettier": "2.4.1",
        "rimraf": "3.0.2",
        "rollup": "2.58.0",
        "rollup-plugin-dts": "4.0.0",
        "rollup-plugin-esbuild": "4.5.0",
        "storybook-builder-vite": "^0.1.1",
        "storybook-dark-mode": "1.0.8",
        "tailwindcss": "2.2.17",
        "typescript": "4.4.4",
        "vite": "^2.5.10"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "dependencies": {
        "@react-aria/accordion": "^3.0.0-alpha.2",
        "@react-aria/aria-modal-polyfill": "^3.4.0",
        "@react-aria/breadcrumbs": "^3.1.5",
        "@react-aria/button": "^3.3.4",
        "@react-aria/checkbox": "^3.2.3",
        "@react-aria/dialog": "^3.1.4",
        "@react-aria/focus": "^3.5.0",
        "@react-aria/interactions": "^3.6.0",
        "@react-aria/link": "^3.1.4",
        "@react-aria/listbox": "^3.3.1",
        "@react-aria/menu": "^3.2.3",
        "@react-aria/overlays": "^3.7.2",
        "@react-aria/radio": "^3.1.5",
        "@react-aria/select": "^3.5.0",
        "@react-aria/table": "^3.0.0",
        "@react-aria/tooltip": "^3.1.3",
        "@react-aria/utils": "^3.9.0",
        "@react-aria/visually-hidden": "^3.2.3",
        "@react-stately/collections": "^3.3.4",
        "@react-stately/menu": "^3.2.3",
        "@react-stately/overlays": "^3.1.3",
        "@react-stately/radio": "^3.3.2",
        "@react-stately/select": "^3.1.3",
        "@react-stately/table": "^3.0.0",
        "@react-stately/toggle": "^3.2.3",
        "@react-stately/tooltip": "^3.0.5",
        "@react-stately/tree": "^3.2.0",
        "deepmerge": "^4.2.2",
        "draft-js": "^0.11.7",
        "framer-motion": "^4.1.17",
        "react-color": "^2.19.3",
        "react-popper": "^2.2.5"
    }
}
