{
    "name": "@frontify/arcade",
    "version": "2.3.0",
    "description": "Design system of Frontify",
    "module": "dist/index.js",
    "files": [
        "dist",
        "tailwind.config.js"
    ],
    "exports": {
        ".": "./dist/index.js",
        "./tailwind": "./tailwind.config.js",
        "./style": "./dist/styles.css",
        "./components/*": {
            "types": "./dist/src/components/*/index.d.ts",
            "import": "./dist/src/components/*/index.js"
        },
        "./types": {
            "types": "./dist/src/types/index.d.ts",
            "import": "./dist/src/types/index.js"
        },
        "./types/*": {
            "types": "./dist/src/types/*.d.ts",
            "import": "./dist/src/types/*.js"
        },
        "./icons": {
            "types": "./dist/src/foundation/Icon/index.d.ts",
            "import": "./dist/src/foundation/Icon/index.js"
        },
        "./icons/*": {
            "types": "./dist/src/foundation/Icon/*.d.ts",
            "import": "./dist/src/foundation/Icon/*.js"
        }
    },
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "npm run clean && npm run generate && cross-env NODE_ENV=production npm run build:library && npm run generate:barrelExport",
        "build:css": "tailwindcss -i ./src/styles.css -o dist/index.css",
        "build:library": "vite build",
        "build:storybook": "build-storybook",
        "dev": "cross-env NODE_ENV=development npm run dev:library",
        "dev:library": "vite build --watch",
        "clean": "rimraf dist",
        "component:create": "esno scripts/createNewComponent.ts",
        "generate": "npm run generate:reactIcons && npm run generate:iconsMap",
        "generate:barrelExport": "esno ./scripts/generateBarrelExport.ts",
        "generate:reactIcons": "esno ./scripts/generateReactIcons.ts",
        "generate:iconsMap": "esno ./scripts/generateIconMap.ts && eslint --fix ./src/foundation/Icon/IconEnum.ts ./src/foundation/Icon/IconsMap.tsx",
        "lint": "npm run lint:ts",
        "lint:fix": "npm run lint:ts:fix",
        "lint:ts": "eslint 'src/**/*.{ts,tsx}'",
        "lint:ts:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
        "lint:prettier": "prettier --check src",
        "test": "npm run build:css && cypress run-ct",
        "test:browser": "npm run build:css && cypress open-ct",
        "test:file": "npm run build:css && cypress run-ct --spec",
        "typecheck": "tsc",
        "storybook": "start-storybook -p 6006",
        "prepublishOnly": "npm run build",
        "prepare": "is-ci || husky install"
    },
    "keywords": [
        "frontify",
        "react",
        "components",
        "design system"
    ],
    "author": "Frontify Developers <developers@frontify.com>",
    "repository": {
        "url": "https://github.com/frontify/arcade"
    },
    "bugs": {
        "url": "https://github.com/frontify/arcade/issues"
    },
    "devDependencies": {
        "@cypress/react": "5.12.1",
        "@cypress/vite-dev-server": "2.2.2",
        "@frontify/eslint-config-react": "0.13.2",
        "@storybook/addon-actions": "6.4.14",
        "@storybook/addon-essentials": "6.4.14",
        "@storybook/addon-links": "6.4.14",
        "@storybook/react": "6.4.14",
        "@svgr/core": "6.2.0",
        "@svgr/plugin-jsx": "6.2.0",
        "@svgr/plugin-prettier": "6.1.2",
        "@svgr/plugin-svgo": "6.2.0",
        "@types/react": "17.0.38",
        "@types/react-dom": "17.0.11",
        "@types/tinycolor2": "1.4.3",
        "@vitejs/plugin-react-refresh": "1.3.6",
        "autoprefixer": "10.4.2",
        "chalk": "5.0.0",
        "cross-env": "7.0.3",
        "cypress": "9.3.1",
        "cypress-real-events": "1.6.0",
        "esbuild": "0.14.13",
        "eslint": "8.7.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "esno": "0.14.0",
        "fast-glob": "3.2.11",
        "husky": "7.0.4",
        "is-ci": "3.0.1",
        "postcss": "8.4.5",
        "postcss-cli": "9.1.0",
        "postcss-nested": "5.0.6",
        "prettier": "2.5.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "rimraf": "3.0.2",
        "storybook-builder-vite": "0.1.13",
        "storybook-dark-mode": "1.0.8",
        "tailwindcss": "3.0.15",
        "ts-node": "10.4.0",
        "typescript": "4.5.5",
        "vite": "2.7.13",
        "vite-plugin-dts": "0.9.9"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "dependencies": {
        "@frontify/arcade-tokens": "1.1.0-next.9",
        "@react-aria/accordion": "^3.0.0-alpha.2",
        "@react-aria/aria-modal-polyfill": "^3.4.0",
        "@react-aria/breadcrumbs": "^3.1.5",
        "@react-aria/button": "^3.3.4",
        "@react-aria/checkbox": "^3.2.3",
        "@react-aria/combobox": "^3.2.1",
        "@react-aria/dialog": "^3.1.4",
        "@react-aria/dnd": "^3.0.0-alpha.3",
        "@react-aria/focus": "^3.5.0",
        "@react-aria/interactions": "^3.6.0",
        "@react-aria/link": "^3.1.4",
        "@react-aria/listbox": "^3.3.1",
        "@react-aria/menu": "^3.2.3",
        "@react-aria/overlays": "^3.7.2",
        "@react-aria/radio": "^3.1.5",
        "@react-aria/select": "^3.5.0",
        "@react-aria/table": "^3.0.0",
        "@react-aria/tooltip": "^3.1.3",
        "@react-aria/utils": "^3.9.0",
        "@react-aria/visually-hidden": "^3.2.3",
        "@react-stately/checkbox": "^3.0.3",
        "@react-stately/collections": "^3.3.4",
        "@react-stately/data": "^3.4.2",
        "@react-stately/dnd": "^3.0.0-alpha.2",
        "@react-stately/list": "^3.3.0",
        "@react-stately/menu": "^3.2.3",
        "@react-stately/overlays": "^3.1.3",
        "@react-stately/radio": "^3.3.2",
        "@react-stately/select": "^3.1.3",
        "@react-stately/table": "^3.1.0",
        "@react-stately/toggle": "^3.2.3",
        "@react-stately/tooltip": "^3.0.5",
        "@react-stately/tree": "^3.2.0",
        "@xstate/immer": "^0.3.0",
        "@xstate/react": "^1.6.3",
        "deepmerge": "^4.2.2",
        "framer-motion": "^6.2.1",
        "react-color": "^2.19.3",
        "react-popper": "^2.2.5",
        "slate": "^0.72.3",
        "slate-history": "^0.66.0",
        "slate-hyperscript": "^0.67.0",
        "slate-react": "^0.72.6",
        "tinycolor2": "^1.4.2",
        "xstate": "^4.28.1"
    }
}
