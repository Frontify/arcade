{
    "name": "@frontify/arcade",
    "version": "0.6.4",
    "description": "Design system of Frontify",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "default": "./dist/index.js"
        },
        "./config": {
            "require": "./dist/config.js",
            "import": "./dist/config.mjs",
            "default": "./dist/config.js"
        },
        "./style": "./dist/index.css"
    },
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "npm run clean && npm run generate && cross-env NODE_ENV=production npm run build:library",
        "build:library": "rollup -c",
        "build:css": "tailwindcss -i ./src/styles.css -o dist/index.css",
        "build:storybook": "build-storybook -s .storybook/assets",
        "clean": "rimraf dist",
        "component:create": "ts-node scripts/createNewComponent.ts",
        "generate": "ts-node scripts/generateExports.ts",
        "lint": "npm run lint:ts",
        "lint:fix": "npm run lint:ts:fix",
        "lint:ts": "eslint 'src/**/*.{ts,tsx}'",
        "lint:ts:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
        "test": "npm run build:css && cypress run-ct",
        "test:browser": "npm run build:css && cypress open-ct",
        "test:file": "npm run build:css && cypress run-ct --spec",
        "typecheck": "tsc",
        "storybook": "start-storybook -p 6006 -s .storybook/assets",
        "prepublishOnly": "npm run build",
        "prepare": "is-ci || husky install"
    },
    "keywords": [
        "frontify",
        "react",
        "components",
        "design system"
    ],
    "author": "Frontify Developers <developers@frontify.com>",
    "repository": {
        "url": "https://github.com/frontify/arcade"
    },
    "bugs": {
        "url": "https://github.com/frontify/arcade/issues"
    },
    "devDependencies": {
        "@cypress/react": "^5.9.2",
        "@cypress/vite-dev-server": "^2.0.3",
        "@frontify/eslint-config-react": "^0.11.0",
        "@rollup/plugin-alias": "^3.1.5",
        "@rollup/plugin-commonjs": "20.0.0",
        "@rollup/plugin-node-resolve": "^13.0.4",
        "@storybook/addon-actions": "^6.3.6",
        "@storybook/addon-essentials": "^6.3.6",
        "@storybook/addon-links": "^6.3.6",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/react": "^6.3.6",
        "@svgr/core": "^5.5.0",
        "@svgr/plugin-jsx": "^5.5.0",
        "@svgr/plugin-prettier": "^5.5.0",
        "@svgr/plugin-svgo": "^5.5.0",
        "@types/react": "^17.0.15",
        "@types/react-dom": "^17.0.9",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "autoprefixer": "^10.3.1",
        "chalk": "^4.1.2",
        "cross-env": "^7.0.3",
        "cypress": "^8.1.0",
        "cypress-plugin-tab": "^1.0.5",
        "esbuild": "^0.12.17",
        "eslint": "^7.31.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "fast-glob": "^3.2.7",
        "husky": "^7.0.1",
        "is-ci": "^3.0.0",
        "postcss": "^8.3.6",
        "postcss-cli": "^8.3.1",
        "postcss-nested": "^5.0.6",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.55.1",
        "rollup-plugin-dts": "^4.0.0",
        "rollup-plugin-esbuild": "^4.5.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.0",
        "storybook-builder-vite": "^0.0.12",
        "storybook-dark-mode": "^1.0.8",
        "tailwindcss": "^2.2.7",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5",
        "vite": "^2.4.4",
        "vite-react-jsx": "^1.1.2"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-popper": "^2.2.5"
    },
    "dependencies": {
        "@react-aria/accordion": "^3.0.0-alpha.2",
        "@react-aria/aria-modal-polyfill": "^3.4.0",
        "deepmerge": "^4.2.2",
        "framer-motion": "^4.1.17",
        "react-aria": "^3.9.0",
        "react-color": "^2.19.3",
        "react-stately": "^3.8.0"
    }
}
