        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "typecheck": "tsc --noEmit"
    },
    "peerDependencies": {
        "react": "^18"
    },
    "devDependencies": {
        "@frontify/eslint-config-react": "^0.17.0",
        "@iconify/tools": "^4.0.0",
        "@types/lodash": "^4.14.202",
        "@types/node": "^18.19.15",
        "@types/react": "^18.2.55",
        "dotenv-cli": "^7.3.0",
        "eslint": "^8.56.0",
        "figma-api": "^1.11.0",
        "lodash": "^4.17.21",
        "prettier": "^3.2.5",
        "react": "^18.2.0",
        "svgson": "^5.3.1",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
